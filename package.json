{
  "name": "n8n-nodes-anysite",
  "version": "1.0.4",
  "description": "n8n node to access AnySite API services",
  "keywords": [
    "n8n-community-node-package",
    "linkedin",
    "twitter",
    "x",
    "reddit",
    "instagram",
    "anysite",
    "social media",
    "data extraction"
  ],
  "license": "MIT",
  "homepage": "https://anysite.io",
  "author": {
    "name": "AnySite",
    "email": "support@anysite.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/horizondatawave/n8n-nodes-hdw.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AnySiteApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AnySite/AnySiteLinkedin.node.js",
      "dist/nodes/AnySite/AnySiteLinkedinManagement.node.js",
      "dist/nodes/AnySite/AnySiteTwitter.node.js",
      "dist/nodes/AnySite/AnySiteWebParser.node.js",
      "dist/nodes/AnySite/AnySiteJobBoards.node.js",
      "dist/nodes/AnySite/AnySiteReddit.node.js",
      "dist/nodes/AnySite/AnySiteInstagram.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.18.10",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "gulp": "^4.0.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
